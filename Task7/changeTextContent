<!DOCTYPE html>
<html>
<head>
	<title>Document</title>
</head>
<body>
	<button id='1'>change</button>
	<p id='2'>Content</p>

	<script>
		let button = document.getElementById("1").addEventListener("click", function chnageContent() {
			let paragraph = document.getElementById("2");
			if ( paragraph.innerHTML === "Content" ) {
				paragraph.innerHTML = "Changed content";
			} else {
				paragraph.innerHTML = "Content";
			}
		});
	</script>
</body>
</html>
